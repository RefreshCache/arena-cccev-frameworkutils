using Arena.Custom.Cccev.FrameworkUtils.Interfaces;

namespace Arena.Custom.Cccev.FrameworkUtils.BLL
{
    public enum CacheType
    {
        Http,
        Dictionary
    }

    public static class CacheFactory
    {
        public static ICachable GetCache(CacheType type)
        {
            switch (type)
            {
                case CacheType.Http:
                    return HttpCache.Instance;
                case CacheType.Dictionary:
                    return DictionaryCache.Instance;
                default:
                    return null;
            }
        }
    }
}
